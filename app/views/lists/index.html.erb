<h1>My Lists</h1>
<% @lists.each do |list| %>
<li>
  <% if list.photo.attached? %>
  <%= cl_image_tag list.photo.key, width: 400, height: 300, crop: :fill %>
  <% end %>
  <%= list.name %>
  <%= link_to "See details", list_path(list), class: "btn btn-primary"%>
</li>
<% end %>
<br>
<br>
<p>
<%= link_to "Create a new list", new_list_path, class: "btn btn-primary"%>
